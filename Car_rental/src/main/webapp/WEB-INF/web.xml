<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<absolute-ordering>
	<web-app>
		<display-name>Archetype Created Web Application</display-name>
		<servlet>
			<servlet-name>Controller</servlet-name>
			<display-name>Controller</display-name>
			<description></description>
			<servlet-class>by.epam.lab.servlet.Controller</servlet-class>
		</servlet>
		<servlet>
			<servlet-name>MainPageServlet</servlet-name>
			<display-name>MainPageServlet</display-name>
			<description></description>
			<servlet-class>by.epam.lab.main_page_servlet.MainPageServlet</servlet-class>
		</servlet>
		<servlet>
			<servlet-name>OrderFormServlet</servlet-name>
			<display-name>OrderFormServlet</display-name>
			<description></description>
			<servlet-class>by.epam.lab.main_page_servlet.OrderFormServlet</servlet-class>
		</servlet>
		<servlet>
			<servlet-name>CarDescriptionServlet</servlet-name>
			<display-name>CarDescriptionServlet</display-name>
			<description></description>
			<servlet-class>by.epam.lab.main_page_servlet.CarDescriptionServlet</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>Controller</servlet-name>
			<url-pattern>/controller</url-pattern>
		</servlet-mapping>
		<servlet-mapping>
			<servlet-name>MainPageServlet</servlet-name>
			<url-pattern>/MainPageServlet</url-pattern>
		</servlet-mapping>
		<servlet-mapping>
			<servlet-name>OrderFormServlet</servlet-name>
			<url-pattern>/OrderFormServlet</url-pattern>
		</servlet-mapping>
		<servlet-mapping>
			<servlet-name>CarDescriptionServlet</servlet-name>
			<url-pattern>/CarDescriptionServlet</url-pattern>
		</servlet-mapping>
		<welcome-file-list>
			<welcome-file>index.jsp</welcome-file>
		</welcome-file-list>
		<jsp-config>
			<taglib>
				<taglib-uri>customtags</taglib-uri>
				<taglib-location>/WEB-INF/tld/custom.tld</taglib-location>
			</taglib>
		</jsp-config>
		<listener>
			<listener-class>ContextListener</listener-class>
		</listener>
	</web-app>
</absolute-ordering>